{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "overrides": [],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "func-names":"off",
        "consistent-return":"off",
        "no-underscore-dangle":"warn",
        "no-shadow":"warn"
    }
}